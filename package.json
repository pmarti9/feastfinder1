{
  "name": "FeastFinder",
  "version": "1.0.0",
  "description": "FeastFinder Backend",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mysql2": "^2.3.0",
    "passport": "^0.5.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.6.5",
    "validator": "^13.6.0"
  }
}
